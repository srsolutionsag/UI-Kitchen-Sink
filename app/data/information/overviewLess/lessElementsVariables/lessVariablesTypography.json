{
  "id": "lessVariablesTypography",
  "title": "Typography",
  "description": "Less Variables used to change fonts and fonts sizes",
  "type": "less",
  "variables":{
      "bootstrap":[
          {
              "title": "font-family-base",
              "description": "Default font",
              "value":"@font-family-sans-serif",
              "state": "Implemented for 5.0"
          },{
              "title": "font-size-base",
              "description": "Bootstrap's global default font-size is 14px, with a line-height of 1.428. This is applied to the <body> and all paragraphs. In addition, <p> (paragraphs) receive a bottom margin of half their computed line-height (10px by default).",
              "value": "14px",
              "state": "Implemented for 5.0"
          },{
              "title": "line-height-base",
              "description": "Unit-less `line-height` for use in components like buttons.",
              "value": "1.428571429 (=20/14)",
              "state": "Implemented for 5.0"
          },{
              "title": "line-height-computed",
              "description": "Computed 'line-height' (`font-size` * `line-height`) for use with `margin`, `padding`, etc.",
              "value": "floor((@font-size-base * @line-height-base))",
              "state": "Implemented for 5.0"
          },
          {
              "title": "font-family-sans-serif",
              "description": "Default font for sans-serfis",
              "value": "@font-family-sans-serif:  'Helvetica Neue', Helvetica, Arial, sans-serif;",
              "state": "Implemented for 5.0"
          },{
              "title": "font-family-serif",
              "description": "Default font for serfis",
              "value": "Georgia, 'Times New Roman', Times, serif",
              "state": "Implemented for 5.0"
          },{
              "title": "font-family-monospace",
              "description": "Default monospace fonts for `<code>`, `<kbd>`, and `<pre>`",
              "value": "Menlo, Monaco, Consolas, 'Courier New', monospace;",
              "state": "Implemented for 5.0"
          },
          {
              "title": "font-size-h1",
              "description": "Size for h1",
              "value":"floor((@font-size-base * 2.6)); // ~36px",
              "state": "Implemented for 5.0"
          },{
              "title": "font-size-h2",
              "description": "Size for h2",
              "value":"floor((@font-size-base * 2.15)); // ~30px",
              "state": "Implemented for 5.0"
          },{
              "title": "font-size-h3",
              "description": "Size for h3",
              "value":"ceil((@font-size-base * 1.7)); // ~24px",
              "state": "Implemented for 5.0"
          },{
              "title": "font-size-h4",
              "description": "Size for h4",
              "value":"ceil((@font-size-base * 1.25)); // ~18px",
              "state": "Implemented for 5.0"
          },
          {
              "title": "font-size-h5",
              "description": "Size for h5",
              "value":"@font-size-base",
              "state": "Implemented for 5.0"
          },{
              "title": "font-size-h6",
              "description": "Size for h6",
              "value":"ceil((@font-size-base * 0.85)); // ~12px",
              "state": "Implemented for 5.0"
          },{
              "title": "headings-font-family",
              "description": "By default, this inherits from the `<body>`",
              "value":"inherit",
              "state": "Implemented for 5.0"
          },{
              "title": "headings-font-weight",
              "description": "Font weight for headings",
              "value":"500",
              "state": "Implemented for 5.0"
          },{
              "title": "headings-line-height",
              "description": "Line height for headings",
              "value":"1.1",
              "state": "Implemented for 5.0"
          },{
              "title": "headings-color",
              "description": "By default, this inherits from the `<body>`",
              "value":"inherit",
              "state": "Implemented for 5.0"
          }
      ]
  }
}